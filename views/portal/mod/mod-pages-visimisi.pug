    .row
        .col-sm-2
            .post-thumb-single
                img.img-responsive(src=base_url+'images/Lambang_Kota_Tanjung_Pinang_GRA.png', alt='Kota Tanjungpinang', width='20px')
                .image-overlay
                a.post-zoom(href=base_url+'images/lambang_235.png', rel='prettyPhoto[pp_pages_profil]')
                    i.fa-eye

            // /.post-thumb-single
        .col-sm-10
            .post-content
                h5.subpage-title
